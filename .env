# Google client secret
TRAEFIK_AUTH_PROVIDERS_GOOGLE_CLIENT_SECRET=uuB3wQKy-IZEfwQF7ZvDoqDs
# Users whitelist (separated by coma)
TRAEFIK_AUTH_WHITELIST=

